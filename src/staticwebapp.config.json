{
   "routes": [
      {
         "route": "/api/*",
         "allowedRoles": [
            "anonymous"
         ],
         "methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE"
         ]
      }
   ],
   "responseOverrides": {
      "404": {
         "rewrite": "/404.html"
      }
   },
   "mimeTypes": {
      ".js": "application/javascript",
      ".css": "text/css",
      ".html": "text/html"
   },
   "platform": {
      "apiRuntime": "dotnet:6.0"
   }
}